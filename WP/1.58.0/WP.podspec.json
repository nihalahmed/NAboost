{
  "name": "WP",
  "version": "1.58.0",
  "summary": "Boost provides free peer-reviewed portable C++ source libraries.",
  "homepage": "http://www.boost.org",
  "authors": "Rene Rivera",
  "source": {
    "git": "https://github.com/Wattpad/WattpadSDK.git",
    "submodules": true
  },
  "platforms": {
    "ios": "4.0"
  },
  "source_files": [
        "generated-srcs/cpp/**/*",
        "generated-srcs/objc/**/*",
        "djinni/support-lib/cpp/**/*",
        "djinni/support-lib/objc/**/*",
        "src/cpp/**/*",
        "jsoncpp/**/*"
  ],
  "prepare_command": "make",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "${PODS_ROOT}/boost ${PODS_ROOT}/WP/jsoncpp"
  },
  "dependencies": {
    "boost/optional": [
      "~> 1.58.0"
    ]
  },
  "libraries": "c++"
}
