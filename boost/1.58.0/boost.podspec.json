{
  "name": "boost",
  "version": "1.58.0",
  "summary": "Boost provides free peer-reviewed portable C++ source libraries.",
  "homepage": "http://www.boost.org",
  "license": {
    "type": "Boost Software License",
    "file": "LICENSE_1_0.txt"
  },
  "authors": "Rene Rivera",
  "source": {
    "http": "http://sourceforge.net/projects/boost/files/boost/1.58.0/boost_1_58_0.tar.gz"
  },
  "platforms": {
    "ios": "4.0",
    "osx": "10.6"
  },
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "\"${PODS_ROOT}/boost\""
  },
  "requires_arc": false,
  "subspecs": [
    {
      "name": "optional",
      "preserve_paths": [
        "boost/*.hpp",
        "boost/{optional}/**/*.hpp",
      ]
    }
  ]
}
